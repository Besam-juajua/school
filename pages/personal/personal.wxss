.profile{
  width: 712rpx;
  height: 400rpx;
  background: #FF9800;
  border-radius: 10rpx;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  text-align: center;
  padding-top: 40rpx;
}
.profile image{
  display: block;
  width: 232rpx;
  height: 232rpx;
  border-radius: 50%;
  margin: 0rpx auto 28rpx;
  border: 2rpx solid #BBBBBB;
  box-sizing: border-box;
}
.profile text{
  font-size: 28rpx;
  line-height: 20rpx;
  color: #101010;
}
.identify-item{
  width: 730rpx;
  height: 120rpx;
  border: 2rpx solid #BBBBBB;
  margin: 0 10rpx 12rpx;
  border-radius: 10rpx;
  line-height: 120rpx;
}
.identify-item .icon1{
  width: 52rpx;
  height: 52rpx;
  float: left;
  margin: 34rpx 28rpx 34rpx 30rpx;
}
.identify-item text{
  font-size: 28rpx;
  float: left;
}
.identify-item .icon2{
  width: 54rpx;
  height: 54rpx;
  float: right;
  margin: 32rpx 16rpx;
}